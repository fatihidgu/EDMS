<!--==========================
    ABOUTUS Section Bizim mekan
============================-->


<section id="aboutus" class="md-padding">
  <div class="container">

    <div class="container">
      <div class="row">
        <div class="col-md-2 text-left"></div>

        <div class="col-md-12 bg-light py-3">
          {{#each workUnits}}
            <hr>
            <h2 onclick="getElementById('{{name}}').classList.toggle('d-block');">-{{name}}</h2>
            <div class="col-md-10 float-right d-none" id="{{name}}">

              {{#each mainProcesses}}
              <hr>
              <h3 onclick="getElementById('{{../name}}{{name}}').classList.toggle('d-block');">-{{name}}</h3>
              <div class="col-md-10 float-right d-none" id="{{../name}}{{name}}">

                {{#each workflows}}
                <hr>
                  <h4 onclick="getElementById('{{../../name}}{{../name}}{{name}}').classList.toggle('d-block');">-{{name}}</h4>
                  <div class="col-md-10 float-right d-none" id="{{../../name}}{{../name}}{{name}}">
                    <hr>
                    {{#each files}}
                      <h5>File:{{name}}-{{id}}</h5>
                      <hr>
                    {{/each}}
                  </div>
                  <div style="clear:both"></div>

                {{/each}}
                <hr>
                </div>
                <div style="clear:both"></div>
                {{/each}}
                <hr>
            </div>
            <div style="clear:both"></div>


          {{/each}}
          <hr>
        </div>
      </div>






